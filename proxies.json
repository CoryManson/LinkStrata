{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "root": {
            "disabled": false,
            "matchCondition": {
                "methods": [
                    "GET"
                ],
                "route": "{*path}"
            },
            "backendUri": "https://localhost/api/ServeStaticFile?file={path}",
            "responseOverrides": {
                "response.headers.Cache-Control": "public,max-age=600"
            }
        }
    }
}